input.FormControl-checkbox[type="checkbox"][disabled] { -webkit-text-fill-color: var(--control-fgColor-disabled, var(--color-primer-fg-disabled)); background-color: var(--control-bgColor-disabled, var(--color-input-disabled-bg)); border-color: var(--control-borderColor-disabled, var(--color-btn-border)); color: var(--control-fgColor-disabled, var(--color-primer-fg-disabled)); cursor: not-allowed; opacity: 1; }

