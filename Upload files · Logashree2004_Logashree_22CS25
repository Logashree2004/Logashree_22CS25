  input.FormControl-checkbox[type="checkbox"]::before { animation: 80ms cubic-bezier(0.65, 0, 0.35, 1) 0s 1 normal forwards running checkmarkOut; }
}

input.FormControl-checkbox[type="checkbox"]::after { content: ""; height: 100%; left: 50%; min-height: var(--control-medium-size, 2rem) var(--control-medium-size, 2rem); position: absolute; top: 50%; transform: translateX(-50%) translateY(-50%); width: 100%; }

