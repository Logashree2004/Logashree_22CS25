input.FormControl-checkbox[type="checkbox"]::before { background-color: var(--control-checked-fgColor-rest, var(--color-switch-track-checked-fg)); clip-path: inset(var(--base-size-16, 1rem) 0 0 0); content: ""; height: var(--base-size-16, 1rem); -webkit-mask-image: url("data:image/svg+xml;base64,"); -webkit-mask-position: center center; -webkit-mask-repeat: no-repeat; -webkit-mask-size: 75%; transition: visibility 0s linear 0.23s; visibility: hidden; width: var(--base-size-16, 1rem); }

